[
  {
    "description": "这是一个带cookie的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "然然带cookies信息访问的get请求"
    }
  },
  {
    "description": "这是一个带cookie的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "ranran",
        "age": "1"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "ranran": "success",
        "status": "1"
      }
    }
  }
]