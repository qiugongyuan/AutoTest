[
  {
    "description": "这是一个带header的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },

      "json": {
        "name": "ranran",
        "age": "1"
      }

    },
    "response": {
      "json": {
        "ranran": "success",
        "status": "1"
      }
    }

  }
]